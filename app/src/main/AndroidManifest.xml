<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.transportprocess2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_laucher_toledo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_laucher_toledo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".sausages.MainSausageActivity"></activity>
        <activity android:name=".MenuActivity"></activity>
        <activity android:name=".profiles.SplashActivity" />
        <activity android:name=".report.ReportActivity" />
        <activity android:name=".profiles.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".storehouse.StoreHouseActivity" />
        <activity android:name=".deliver.DeliverActivity"></activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>